# Backend API URL
VITE_BACKEND_URL=http://localhost:3000

# Google OAuth 2.0 Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here
VITE_GOOGLE_REDIRECT_URI=http://localhost:8080

# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:5158/ws

# Authentication (if using external auth providers)
VITE_AUTH_DOMAIN=your-auth-domain
VITE_AUTH_CLIENT_ID=your-client-id
VITE_AUTH_AUDIENCE=your-audience

# Feature Flags
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_TYPING_INDICATOR=true
VITE_ENABLE_MESSAGE_POLLING=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info 